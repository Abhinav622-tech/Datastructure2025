#include <iostream>
using namespace std;

class Queue {
    int arr[100];
    int front, rear, size;
public:
    Queue() {
        front = 0;
        rear = -1;
        size = 0;
    }

    bool isEmpty() { return size == 0; }
    bool isFull() { return size == 100; }

    void enqueue(int x) {
        if (isFull()) {
            cout << "Queue Full\n";
            return;
        }
        arr[++rear] = x;
        size++;
    }

    void dequeue() {
        if (isEmpty()) {
            cout << "Queue Empty\n";
            return;
        }
        cout << "Dequeued: " << arr[front] << "\n";
        front++;
        size--;
    }

    void peek() {
        if (isEmpty()) cout << "Queue Empty\n";
        else cout << "Front: " << arr[front] << "\n";
    }

    void display() {
        if (isEmpty()) {
            cout << "Queue Empty\n";
            return;
        }
        cout << "Queue: ";
        for (int i = front; i <= rear; i++) cout << arr[i] << " ";
        cout << "\n";
    }
};

int main() {
    Queue q;
    int ch, x;

    while (true) {
        cout << "\n1.Enqueue\n2.Dequeue\n3.Peek\n4.Display\n5.isEmpty\n6.isFull\n0.Exit\n";
        cin >> ch;

        switch (ch) {
            case 1: cin >> x; q.enqueue(x); break;
            case 2: q.dequeue(); break;
            case 3: q.peek(); break;
            case 4: q.display(); break;
            case 5: cout << (q.isEmpty() ? "Empty\n" : "Not Empty\n"); break;
            case 6: cout << (q.isFull() ? "Full\n" : "Not Full\n"); break;
            case 0: return 0;
        }
    }
}

